<table class="table table-hover table-stripped table-dark">
  <thead>
    <th>E-mail</th>
    <th>Permisos</th>
    <th>Última Sesión</th>
    <th>Opciones</th>
  </thead>
  <tbody>
    <% @admins.each do |a| %>
    <tr>
      <td class="align-middle"><%= a.get_email %></td>
      <td class="align-middle"><%= a.get_permissions %></td>
      <td class="align-middle"><%= a.get_last_session %></td>
      <td class="align-middle">...</td>
    </tr>
    <% end %>
  </tbody>
</table>
