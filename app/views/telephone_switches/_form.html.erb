<div class="modal fade" id="form-telephone-switch-modal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
<div class="modal-dialog" role="document">
  <div class="modal-content">
    <div class="modal-header">
      <h5 class="modal-title" id="exampleModalLabel">Formulario Central Telefónica</h5>
      <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
    </div>

    <%= form_with(model: telephone_switch, url:url, method:method) do |o| %>

      <div class="modal-body">

        <div class="form-group">
          <%= o.label :name, "Central Telefónica" %>
          <%= o.text_field :name, class:"form-control" %>
        </div>

        <div class="form-group">
          <%= o.label :header, "Cabecera" %>
          <%= o.text_field :header, class:"form-control" %>
        </div>

        <div class="form-group">
          <%= o.label :ip, "Dir. IP" %>
          <%= o.text_field :ip, class:"form-control" %>
        </div>

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
        <%= o.submit "Guardar", class:"btn btn-primary" %>
      </div>

    <% end %>

  </div>
</div>
</div>
